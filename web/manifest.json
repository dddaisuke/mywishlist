{
  "name": "My wish list",
  "author": "Daisuke Yamashita (@dddaisuke)",
  "description": "This is a Chrome extension for Google Forms.",
  "icons": {
    "16": "images/16x16.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "browser_action": {
    "default_icon": "images/32x32.png",
    "default_popup": "mywishlist.html"
  },
  "permissions": [
    "alarms",
    "https://docs.google.com/"
  ],
  "version": "0.0.1",
  "manifest_version": 2,
  "offline_enabled": false
}
